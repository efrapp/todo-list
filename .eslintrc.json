{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "airbnb-base",
        "eslint:recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "settings": {
        "import/core-modules": ["bootstrap", "date-fns", "bootstrap/dist/css"]
    },
    "rules": {
        "no-shadow": 0,
        "no-param-reassign": 0,
        "eol-last": 0,
        "arrow-parens": [2, "as-needed", { "requireForBlockBody": true }]
    }
}